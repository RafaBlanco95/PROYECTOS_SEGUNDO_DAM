<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient"
    tools:context=".MyProfileActivity">

    <LinearLayout
        android:id="@+id/editlayout"
        android:layout_width="411dp"
        android:layout_height="98dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.902"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.331">

        <TextView
            android:id="@+id/changePassword"
            android:layout_width="228dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/holtwood_one_sc"
            android:onClick="goToChangePass"
            android:text="@string/change_password"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/deleteUser"
            android:layout_width="wrap_content"
            android:layout_height="88dp"
            android:fontFamily="@font/holtwood_one_sc"
            android:onClick="checkDeleteUser"
            android:text="@string/delete_user"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25sp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="328dp"
        android:layout_height="56dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.029"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.929">

        <TextView
            android:id="@+id/userNameProfileHead"
            android:layout_width="177dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/holtwood_one_sc"
            android:labelFor="@id/usuarioRegistro"
            android:text="@string/usuario_nombre"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/userNameProfileName"
            android:layout_width="146dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/holtwood_one_sc"
            android:textAlignment="center"
            android:textColor="@color/purple_500"
            android:textSize="25sp" />

    </LinearLayout>

    <FrameLayout
        android:id="@+id/frameLayout5"
        android:layout_width="170dp"
        android:layout_height="134dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.092"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.407">

        <ImageView
            android:id="@+id/iv1"
            android:layout_width="159dp"
            android:layout_height="119dp"
            android:contentDescription="@string/my_profile"
            android:onClick="tomarFoto"
            app:srcCompat="@mipmap/perfil" />
    </FrameLayout>

    <TextView
        android:id="@+id/textView"
        android:layout_width="224dp"
        android:layout_height="59dp"
        android:fontFamily="@font/holtwood_one_sc"
        android:text="@string/my_profile"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.049"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.045" />

    <FrameLayout
        android:layout_width="105dp"
        android:layout_height="80dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.061"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.646">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="95dp"
            android:layout_height="65dp"
            android:contentDescription="@string/my_profile"
            android:onClick="tomarFoto"
            app:srcCompat="@mipmap/camara" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="112dp"
        android:layout_height="109dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.573"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.917">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="match_parent"
            android:layout_height="94dp"
            android:contentDescription="@string/change_password"
            android:onClick="goToChangePass"
            app:srcCompat="@mipmap/clave" />
    </FrameLayout>

    <FrameLayout
        android:layout_width="123dp"
        android:layout_height="95dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.899"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.876">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="112dp"
            android:layout_height="match_parent"
            android:contentDescription="@string/delete_user"
            android:onClick="checkDeleteUser"
            app:srcCompat="@mipmap/basura" />
    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>